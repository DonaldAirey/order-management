<Window x:Class="Teraque.Windows.MainWindow"
		xmlns:component="clr-namespace:System.ComponentModel;assembly=WindowsBase"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		Icon="Resources\ColumnView Sandbox.ico"
		Focusable="True"
		Height="350"
		mc:Ignorable="d"
		xmlns:local="clr-namespace:Teraque.Windows"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		x:Name="MyMainWindow"
		xmlns:sys="clr-namespace:System;assembly=mscorlib"
		xmlns:system="clr-namespace:System;assembly=mscorlib"
		Title="MainWindow"
		Width="525"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
	<Window.Resources>
		<!-- BrokerIcon -->
		<ObjectDataProvider x:Key="BrokerIcon"
							ObjectType="{x:Type IconSource}">
			<ObjectDataProvider.ConstructorParameters>
				<system:String></system:String>
			</ObjectDataProvider.ConstructorParameters>
		</ObjectDataProvider>
		<Style BasedOn="{StaticResource {ComponentResourceKey TypeInTargetAssembly={x:Type ColumnViewColumnHeader}, ResourceId=ColumnViewColumnHeaderStyle}}"
			   x:Key="columnHeaderStyle"
			   TargetType="{x:Type ColumnViewColumnHeader}">
			<Setter Property="FontFamily"
					Value="Ariel"/>
			<Setter Property="FontSize"
					Value="24"/>
		</Style>
	</Window.Resources>
	<Window.CommandBindings>
		<CommandBinding Command="Commands.FitColumn"
						Executed="OnFitColumn"/>
		<CommandBinding Command="Commands.FitAllColumns"
						Executed="OnFitAllColumns"/>
		<CommandBinding Command="Commands.Filter"
						Executed="OnFilter"/>
	</Window.CommandBindings>
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"/>
			<RowDefinition/>
		</Grid.RowDefinitions>
		<Menu>
			<MenuItem Header="Action">
				<MenuItem Click="OnChangeHorizontalAlignmentClick"
						  Header="Change Horizontal Alignment"/>
				<MenuItem Click="OnChangeHeaderClick"
						  Header="Change Header"/>
				<MenuItem Click="OnChangeHeaderBindingClick"
						  Header="Change Header Binding"/>
				<MenuItem Click="OnChangeStringFormat"
						  Header="Change String Format"/>
				<MenuItem Click="OnSetAllColumnsStyleClick"
						  Header="Set All Columns Style"/>
				<MenuItem Click="OnSetColumnStyleClick"
						  Header="Set Column Style"/>
				<MenuItem Click="OnChangeCityClick"
						  Header="Change City"/>
			</MenuItem>
		</Menu>
		<ListViewport d:DataContext="{d:DesignInstance Type=local:ConsumerCollection, IsDesignTimeCreatable=True}"
				  Grid.Row="1"
				  ItemsSource="{Binding View}"
				  x:Name="ListView">
			<ListView.View>
				<ColumnView AllowsColumnReorder="true"
							ColumnHeaderStringFormat="ddd"
							ColumnHeaderToolTip="Consumer Information"
							x:Name="ColumnView">
					<ColumnViewColumn DisplayMemberBinding="{Binding Path=FirstName}"
									  Header="First"
									  HeaderVerticalAlignment="Bottom"
									  Ordinal="0"
									  Width="80">
						<ColumnViewColumn.Filters>
							<FilterItem Content="A - I"
										GroupName="FirstNameGroup"
										x:Name="FilterFirstNameAtoI"/>
							<FilterItem Content="J - P"
										GroupName="FirstNameGroup"
										x:Name="FilterFirstNameJtoP"/>
							<FilterItem Content="R - Z"
										GroupName="FirstNameGroup"
										x:Name="FilterFirstNameRtoZ"/>
						</ColumnViewColumn.Filters>
					</ColumnViewColumn>
					<ColumnViewColumn HeaderHorizontalAlignment="Center"
									  Ordinal="1"
									  Width="100">
						<ColumnViewColumn.Header>
							<PixelSnapper>
								<Image Height="20"
									   HorizontalAlignment="Center"
									   Source="{Binding Path=SmallImage, Source={StaticResource BrokerIcon}}"
									   VerticalAlignment="Center"
									   Width="20"/>
							</PixelSnapper>
						</ColumnViewColumn.Header>
						<ColumnViewColumn.Filters>
							<FilterItem Content="A - G"
										GroupName="LastNameGroup"
										x:Name="FilterLastNameAtoG"/>
							<FilterItem Content="H - M"
										GroupName="LastNameGroup"
										x:Name="FilterLastNameHtoM"/>
							<FilterItem Content="N - R"
										GroupName="LastNameGroup"
										x:Name="FilterLastNameNtoR"/>
							<FilterItem Content="S - Z"
										GroupName="LastNameGroup"
										x:Name="FilterLastNameStoZ"/>
						</ColumnViewColumn.Filters>
						<ColumnViewColumn.CellTemplate>
							<DataTemplate>
								<Border Background="#10FF00FF">
									<TextBlock Text="{Binding LastName}"/>
								</Border>
							</DataTemplate>
						</ColumnViewColumn.CellTemplate>
					</ColumnViewColumn>
					<ColumnViewColumn DisplayMemberBinding="{Binding Path=Address}"
									  Header="Address"
									  HeaderHorizontalAlignment="Right"
									  HeaderToolTip="The Consumer's Address"
									  HeaderVerticalAlignment="Top"
									  Ordinal="2"
									  Width="100">
						<ColumnViewColumn.HeaderContextMenu>
							<ContextMenu>
								<MenuItem Header="Sort"/>
							</ContextMenu>
						</ColumnViewColumn.HeaderContextMenu>
					</ColumnViewColumn>
					<ColumnViewColumn DisplayMemberBinding="{Binding Path=City}"
									  Header="City"
									  Ordinal="3"
									  Width="80"/>
					<ColumnViewColumn DisplayMemberBinding="{Binding Path=State}"
									  HeaderContainerStyle="{StaticResource columnHeaderStyle}">
						<ColumnViewColumn.Header>
							<system:DateTime>2012/3/15 13:6:55</system:DateTime>
						</ColumnViewColumn.Header>
					</ColumnViewColumn>
					<ColumnViewColumn DisplayMemberBinding="{Binding Path=ZipCode}"
									  Header="Zip"
									  IsVisible="False"
									  Width="80"/>
					<ColumnViewColumn DisplayMemberBinding="{Binding Path=DateOfBirth}"
									  Header="DOB"
									  IsVisible="False"
									  Width="70"/>
				</ColumnView>
			</ListView.View>
		</ListViewport>
	</Grid>
</Window>
