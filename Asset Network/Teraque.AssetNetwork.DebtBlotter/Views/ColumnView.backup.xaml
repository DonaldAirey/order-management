<ViewContainer xmlns:asset="http://schemas.teraque.com/winfx/2006/xaml/asset/presentation"
			   xmlns:options="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
			   xmlns:system="clr-namespace:System;assembly=mscorlib"
			   xmlns:theme="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero"
			   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
	<ViewContainer.Resources>
		<!-- StatusIcon -->
		<ObjectDataProvider x:Key="StatusIcon"
							ObjectType="{x:Type IconSource}">
			<ObjectDataProvider.ConstructorParameters>
				<system:String></system:String>
			</ObjectDataProvider.ConstructorParameters>
		</ObjectDataProvider>
		<!-- PriceUpBrush -->
		<LinearGradientBrush EndPoint="0,1"
							 options:Freeze="True"
							 x:Key="PriceUpBrush"
							 StartPoint="0,0">
			<LinearGradientBrush.GradientStops>
				<GradientStop Color="#00FF00"
							  Offset="0"/>
				<GradientStop Color="#00C000"
							  Offset="1"/>
			</LinearGradientBrush.GradientStops>
		</LinearGradientBrush>
		<!-- PriceDownBrush -->
		<LinearGradientBrush EndPoint="0,1"
							 options:Freeze="True"
							 x:Key="PriceDownBrush"
							 StartPoint="0,0">
			<LinearGradientBrush.GradientStops>
				<GradientStop Color="#FF0000"
							  Offset="0"/>
				<GradientStop Color="#C00000"
							  Offset="1"/>
			</LinearGradientBrush.GradientStops>
		</LinearGradientBrush>
		<!-- FieldMouseOverBorderBrush -->
		<SolidColorBrush Color="#919397"
						 options:Freeze="True"
						 x:Key="FieldMouseOverBorderBrush"/>
		<!-- FieldMouseOverBackgroundBrush -->
		<SolidColorBrush Color="#F5F8FC"
						 options:Freeze="True"
						 x:Key="FieldMouseOverBackgroundBrush"/>
		<!-- FieldSelectedBorderBrush -->
		<SolidColorBrush Color="#919397"
						 options:Freeze="True"
						 x:Key="FieldSelectedBorderBrush"/>
		<!-- FieldSelectedBackgroundBrush -->
		<SolidColorBrush Color="White"
						 options:Freeze="True"
						 x:Key="FieldSelectedBackgroundBrush"/>
		<!-- ItemFocusBackgroundBrush -->
		<LinearGradientBrush EndPoint="0.0,1.0"
							 options:Freeze="True"
							 x:Key="ItemFocusBackgroundBrushKey"
							 StartPoint="0.0,0.0">
			<LinearGradientBrush.GradientStops>
				<GradientStop Color="#DCEBFC"
							  Offset="0.0"/>
				<GradientStop Color="#C1DBFC"
							  Offset="1.0"/>
			</LinearGradientBrush.GradientStops>
		</LinearGradientBrush>
		<!-- ItemFocusInnerBorderBrushKey -->
		<LinearGradientBrush EndPoint="0.0,1.0"
							 options:Freeze="True"
							 x:Key="ItemFocusInnerBorderBrushKey"
							 StartPoint="0.0,0.0">
			<LinearGradientBrush.GradientStops>
				<GradientStop Color="#EBF4FD"
							  Offset="0.0"/>
				<GradientStop Color="#DBEAFD"
							  Offset="1.0"/>
			</LinearGradientBrush.GradientStops>
		</LinearGradientBrush>
		<!-- ItemFocusOuterBorderBrushKey -->
		<SolidColorBrush Color="#7A9FCB"
						 options:Freeze="True"
						 x:Key="ItemFocusOuterBorderBrushKey"/>
		<!-- ItemSelectBackgroundBrush -->
		<LinearGradientBrush EndPoint="0.0,1.0"
							 options:Freeze="True"
							 x:Key="ItemSelectBackgroundBrushKey"
							 StartPoint="0.0,0.0">
			<LinearGradientBrush.GradientStops>
				<GradientStop Color="#F8F8F8"
							  Offset="0.0"/>
				<GradientStop Color="#E5E5E5"
							  Offset="1.0"/>
			</LinearGradientBrush.GradientStops>
		</LinearGradientBrush>
		<!-- ItemMouseOverBackgroundBrush -->
		<LinearGradientBrush EndPoint="0.0,1.0"
							 options:Freeze="True"
							 x:Key="ItemMouseOverBackgroundBrushKey"
							 StartPoint="0.0,0.0">
			<LinearGradientBrush.GradientStops>
				<GradientStop Color="#FAFBFD"
							  Offset="0.0"/>
				<GradientStop Color="#EBF3FD"
							  Offset="1.0"/>
			</LinearGradientBrush.GradientStops>
		</LinearGradientBrush>
		<!-- ItemMouseOverInnerBorderBrush -->
		<LinearGradientBrush EndPoint="0.0,1.0"
							 options:Freeze="True"
							 x:Key="ItemMouseOverInnerBorderBrushKey"
							 StartPoint="0.0,0.0">
			<LinearGradientBrush.GradientStops>
				<GradientStop Color="#FCFDFE"
							  Offset="0.0"/>
				<GradientStop Color="#F2F7FE"
							  Offset="1.0"/>
			</LinearGradientBrush.GradientStops>
		</LinearGradientBrush>
		<!-- ItemMouseOverOuterBorderBrush -->
		<SolidColorBrush Color="#B8D6FB"
						 options:Freeze="True"
						 x:Key="ItemMouseOverOuterBorderBrushKey"/>
		<!-- ItemSelectInnerBorderBrush -->
		<LinearGradientBrush EndPoint="0.0,1.0"
							 options:Freeze="True"
							 x:Key="ItemSelectInnerBorderBrushKey"
							 StartPoint="0.0,0.0">
			<LinearGradientBrush.GradientStops>
				<GradientStop Color="#FAFAFB"
							  Offset="0.0"/>
				<GradientStop Color="#F0F0F0"
							  Offset="1.0"/>
			</LinearGradientBrush.GradientStops>
		</LinearGradientBrush>
		<!-- ItemSelectOuterBorderBrush -->
		<SolidColorBrush Color="#D0D0D0"
						 options:Freeze="True"
						 x:Key="ItemSelectOuterBorderBrushKey"/>
		<!-- ImageHeaderStyle -->
		<Style BasedOn="{StaticResource {ComponentResourceKey TypeInTargetAssembly={x:Type ColumnViewColumnHeader}, ResourceId=ColumnViewColumnHeaderStyle}}"
			   x:Key="ImageHeaderStyle"
			   TargetType="{x:Type ColumnViewColumnHeader}">
			<Setter Property="Padding"
					Value="2"/>
		</Style>
		<!-- ColumnViewItemContainer Style -->
		<Style x:Key="ColumnViewItemContainerStyleKey"
			   TargetType="{x:Type ListViewItem}">
			<Setter Property="Background"
					Value="White"/>
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="{x:Type ListViewItem}">
						<Grid Background="{TemplateBinding Background}">
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto"/>
								<ColumnDefinition/>
							</Grid.ColumnDefinitions>
							<Border CornerRadius="2"
									Name="BackgroundBorder"/>
							<ColumnViewRowPresenter Columns="{Binding Path=(ColumnView.ColumnCollection), RelativeSource={RelativeSource TemplatedParent}}"/>
							<Border BorderThickness="1"
									CornerRadius="2"
									Name="OuterBorder">
								<Border BorderThickness="1"
										CornerRadius="2"
										Name="InnerBorder"/>
							</Border>
						</Grid>
						<ControlTemplate.Triggers>
							<DataTrigger Binding="{Binding IsEven}"
										 Value="true">
								<Setter Property="Background"
										TargetName="BackgroundBorder"
										Value="Beige"/>
							</DataTrigger>
							<MultiTrigger>
								<MultiTrigger.Conditions>
									<Condition Property="IsSelected"
											   Value="true"/>
									<Condition Property="IsFocused"
											   Value="true"/>
								</MultiTrigger.Conditions>
								<Setter Property="BorderBrush"
										TargetName="OuterBorder"
										Value="{StaticResource ItemFocusOuterBorderBrushKey}"/>
								<Setter Property="BorderBrush"
										TargetName="InnerBorder"
										Value="{StaticResource ItemFocusInnerBorderBrushKey}"/>
								<Setter Property="Background"
										TargetName="BackgroundBorder"
										Value="{StaticResource ItemFocusBackgroundBrushKey}"/>
							</MultiTrigger>
							<MultiTrigger>
								<MultiTrigger.Conditions>
									<Condition Property="IsSelected"
											   Value="true"/>
									<Condition Property="IsFocused"
											   Value="false"/>
								</MultiTrigger.Conditions>
								<Setter Property="BorderBrush"
										TargetName="OuterBorder"
										Value="{StaticResource ItemSelectOuterBorderBrushKey}"/>
								<Setter Property="BorderBrush"
										TargetName="InnerBorder"
										Value="{StaticResource ItemSelectInnerBorderBrushKey}"/>
								<Setter Property="Background"
										TargetName="BackgroundBorder"
										Value="{StaticResource ItemSelectBackgroundBrushKey}"/>
							</MultiTrigger>
							<MultiTrigger>
								<MultiTrigger.Conditions>
									<Condition Property="IsSelected"
											   Value="false"/>
									<Condition Property="IsMouseOver"
											   Value="true"/>
								</MultiTrigger.Conditions>
								<Setter Property="BorderBrush"
										TargetName="OuterBorder"
										Value="{StaticResource ItemMouseOverOuterBorderBrushKey}"/>
								<Setter Property="BorderBrush"
										TargetName="InnerBorder"
										Value="{StaticResource ItemMouseOverInnerBorderBrushKey}"/>
								<Setter Property="Background"
										TargetName="BackgroundBorder"
										Value="{StaticResource ItemMouseOverBackgroundBrushKey}"/>
							</MultiTrigger>
							<MultiTrigger>
								<MultiTrigger.Conditions>
									<Condition Property="IsSelected"
											   Value="true"/>
									<Condition Property="IsMouseOver"
											   Value="true"/>
								</MultiTrigger.Conditions>
								<Setter Property="BorderBrush"
										TargetName="OuterBorder"
										Value="{StaticResource ItemFocusOuterBorderBrushKey}"/>
								<Setter Property="BorderBrush"
										TargetName="InnerBorder"
										Value="{StaticResource ItemFocusInnerBorderBrushKey}"/>
								<Setter Property="Background"
										TargetName="BackgroundBorder"
										Value="{StaticResource ItemFocusBackgroundBrushKey}"/>
							</MultiTrigger>
						</ControlTemplate.Triggers>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
		<!-- CommonStyle -->
		<Style x:Key="CommonStyle">
			<Setter Property="TextElement.FontFamily"
					Value="Segoe UI"/>
			<Setter Property="TextElement.FontSize"
					Value="12"/>
			<Style.Triggers>
				<DataTrigger Binding="{Binding IsActive}"
							 Value="false">
					<Setter Property="TextElement.Foreground"
							Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}"/>
				</DataTrigger>
			</Style.Triggers>
		</Style>
		<!-- CellStyle -->
		<Style BasedOn="{StaticResource CommonStyle}"
			   x:Key="CellStyle">
			<Setter Property="Control.IsTabStop"
					Value="false"/>
			<Setter Property="Control.Focusable"
					Value="false"/>
			<Setter Property="Control.BorderThickness"
					Value="0,0,1,0"/>
			<Setter Property="Control.BorderBrush"
					Value="#D0D7E5"/>
			<Setter Property="Control.Padding"
					Value="2"/>
		</Style>
		<!-- ValueStyle -->
		<Style BasedOn="{StaticResource CellStyle}"
			   x:Key="ValueStyle"
			   TargetType="{x:Type ContentControl}">
			<Setter Property="Control.Padding"
					Value="2,2,2,3"/>
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="{x:Type ContentControl}">
						<UserControl Background="{TemplateBinding Background}"
									 BorderBrush="{TemplateBinding BorderBrush}"
									 BorderThickness="{TemplateBinding BorderThickness}">
							<ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
											  Margin="{TemplateBinding Padding}"
											  VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
						</UserControl>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
		<!-- ComboBoxStyle -->
		<Style BasedOn="{StaticResource CellStyle}"
			   x:Key="ComboBoxStyle"
			   TargetType="ComboBox">
			<Setter Property="IsSynchronizedWithCurrentItem"
					Value="False"/>
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="{x:Type ComboBox}">
						<UserControl Background="{TemplateBinding Background}"
									 BorderBrush="{TemplateBinding BorderBrush}"
									 BorderThickness="{TemplateBinding BorderThickness}"
									 Name="Frame">
							<Grid>
								<ContentPresenter Content="{TemplateBinding SelectionBoxItem}"
												  ContentTemplate="{TemplateBinding SelectionBoxItemTemplate}"
												  ContentTemplateSelector="{TemplateBinding ItemTemplateSelector}"
												  HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
												  IsHitTestVisible="false"
												  Margin="{TemplateBinding Padding}"
												  VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
								<ToggleButton ClickMode="Press"
											  Focusable="True"
											  FocusVisualStyle="{x:Null}"
											  IsChecked="{Binding Path=IsDropDownOpen,Mode=TwoWay,RelativeSource={RelativeSource TemplatedParent}}"
											  IsTabStop="false">
									<ToggleButton.Template>
										<ControlTemplate TargetType="{x:Type ToggleButton}">
											<Grid Background="Transparent"/>
										</ControlTemplate>
									</ToggleButton.Template>
								</ToggleButton>
								<Popup AllowsTransparency="true"
									   Grid.ColumnSpan="3"
									   IsOpen="{Binding Path=IsDropDownOpen, RelativeSource={RelativeSource TemplatedParent}}"
									   Margin="1"
									   Name="PART_Popup"
									   Placement="Bottom"
									   PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}">
									<theme:SystemDropShadowChrome Color="Transparent"
																  MaxHeight="{TemplateBinding MaxDropDownHeight}"
																  MinWidth="{Binding ElementName=Frame,Path=ActualWidth}"
																  Name="DropShadow">
										<Border Background="{DynamicResource {x:Static SystemColors.WindowBrushKey}}"
												BorderBrush="{DynamicResource {x:Static SystemColors.WindowFrameBrushKey}}"
												BorderThickness="1"
												Name="DropDownBorder">
											<ItemsPresenter KeyboardNavigation.DirectionalNavigation="Contained"/>
										</Border>
									</theme:SystemDropShadowChrome>
								</Popup>
							</Grid>
						</UserControl>
						<ControlTemplate.Triggers>
							<Trigger Property="Popup.HasDropShadow"
									 SourceName="PART_Popup"
									 Value="true">
								<Setter Property="Margin"
										TargetName="DropShadow"
										Value="0,0,5,5"/>
								<Setter Property="Color"
										TargetName="DropShadow"
										Value="#71000000"/>
							</Trigger>
							<MultiTrigger>
								<MultiTrigger.Conditions>
									<Condition Property="IsMouseOver"
											   Value="True"/>
									<Condition Property="IsKeyboardFocusWithin"
											   Value="False"/>
								</MultiTrigger.Conditions>
								<Setter Property="Background"
										Value="{StaticResource FieldMouseOverBackgroundBrush}"/>
							</MultiTrigger>
							<Trigger Property="IsKeyboardFocusWithin"
									 Value="True">
								<Setter Property="Background"
										Value="{StaticResource FieldSelectedBackgroundBrush}"/>
							</Trigger>
						</ControlTemplate.Triggers>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
		<!-- DateStyle -->
		<Style BasedOn="{StaticResource ValueStyle}"
			   x:Key="DateStyle"
			   TargetType="{x:Type ContentControl}">
			<Setter Property="ContentStringFormat"
					Value="{}{0:MM/dd}"/>
			<Setter Property="HorizontalContentAlignment"
					Value="Right"/>
			<Setter Property="VerticalContentAlignment"
					Value="Center"/>
		</Style>
		<!-- ItemSelectorStyle -->
		<Style BasedOn="{StaticResource CellStyle}"
			   x:Key="ItemSelectorStyle"
			   TargetType="ItemSelector">
			<Setter Property="HorizontalContentAlignment"
					Value="Center"/>
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="{x:Type ItemSelector}">
						<UserControl Background="{TemplateBinding Background}"
									 BorderBrush="{TemplateBinding BorderBrush}"
									 BorderThickness="{TemplateBinding BorderThickness}">
							<Grid>
								<ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
												  VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
							</Grid>
						</UserControl>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
		<!-- LeftTextStyle -->
		<Style BasedOn="{StaticResource ValueStyle}"
			   x:Key="LeftTextStyle"
			   TargetType="{x:Type ContentControl}">
			<Setter Property="ContentStringFormat"
					Value="{}{0}"/>
			<Setter Property="HorizontalContentAlignment"
					Value="Left"/>
			<Setter Property="VerticalContentAlignment"
					Value="Center"/>
		</Style>
		<!-- MarketValueStyle -->
		<Style BasedOn="{StaticResource ValueStyle}"
			   x:Key="MarketValueStyle"
			   TargetType="{x:Type ContentControl}">
			<Setter Property="ContentStringFormat"
					Value="{}{0:#,##0}"/>
			<Setter Property="HorizontalContentAlignment"
					Value="Right"/>
			<Setter Property="VerticalContentAlignment"
					Value="Center"/>
		</Style>
		<!-- MaturityDateStyle -->
		<Style BasedOn="{StaticResource ValueStyle}"
			   x:Key="MaturityDateStyle"
			   TargetType="{x:Type ContentControl}">
			<Setter Property="ContentStringFormat"
					Value="{}{0:MM/yy}"/>
			<Setter Property="HorizontalContentAlignment"
					Value="Right"/>
			<Setter Property="VerticalContentAlignment"
					Value="Center"/>
		</Style>
		<!-- PercentStyle -->
		<Style BasedOn="{StaticResource ValueStyle}"
			   x:Key="PercentStyle"
			   TargetType="{x:Type ContentControl}">
			<Setter Property="ContentStringFormat"
					Value="{}{0:0.00;-0.00}"/>
			<Setter Property="HorizontalContentAlignment"
					Value="Right"/>
			<Setter Property="VerticalContentAlignment"
					Value="Center"/>
		</Style>
		<!-- PriceStyle -->
		<Style BasedOn="{StaticResource CellStyle}"
			   x:Key="PriceStyle"
			   TargetType="{x:Type PriceControl}">
			<Setter Property="TickDownBackground"
					Value="{StaticResource PriceDownBrush}"/>
			<Setter Property="TickDuration"
					Value="00:00:10.0"/>
			<Setter Property="TickOpacity"
					Value="0.6"/>
			<Setter Property="TickUpBackground"
					Value="{StaticResource PriceUpBrush}"/>
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="{x:Type PriceControl}">
						<UserControl BorderBrush="{TemplateBinding BorderBrush}"
									 BorderThickness="{TemplateBinding BorderThickness}">
							<Grid>
								<UserControl Background="{TemplateBinding MaskBackground}"
											 Opacity="{TemplateBinding MaskOpacity}"/>
								<UserControl Padding="{TemplateBinding Padding}">
									<ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
													  TextElement.Foreground="{TemplateBinding Foreground}"
													  VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
								</UserControl>
							</Grid>
						</UserControl>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
		<!-- QuantityStyle -->
		<Style BasedOn="{StaticResource ValueStyle}"
			   x:Key="QuantityStyle"
			   TargetType="{x:Type ContentControl}">
			<Setter Property="ContentStringFormat"
					Value="{}{0:#,##0}"/>
			<Setter Property="HorizontalContentAlignment"
					Value="Right"/>
			<Setter Property="VerticalContentAlignment"
					Value="Center"/>
		</Style>
		<!-- ShortDateStyle -->
		<Style BasedOn="{StaticResource ValueStyle}"
			   x:Key="ShortDateStyle"
			   TargetType="{x:Type ContentControl}">
			<Setter Property="ContentStringFormat"
					Value="{}{0:MM/dd hh:mm}"/>
			<Setter Property="HorizontalContentAlignment"
					Value="Right"/>
			<Setter Property="VerticalContentAlignment"
					Value="Center"/>
		</Style>
		<!-- StatusItem -->
		<DataTemplate DataType="{x:Type asset:StatusItem}">
			<Grid>
				<Image Height="18"
					   HorizontalAlignment="Center"
					   Source="{Binding Path=MediumImageSource}"
					   VerticalAlignment="Center"
					   Width="18"/>
			</Grid>
		</DataTemplate>
		<!-- SideStyle -->
		<Style BasedOn="{StaticResource ComboBoxStyle}"
			   x:Key="SideStyle"
			   TargetType="asset:SideComboBox">
			<Setter Property="HorizontalContentAlignment"
					Value="Center"/>
			<Setter Property="ItemTemplate">
				<Setter.Value>
					<DataTemplate>
						<Grid>
							<Image Height="18"
								   HorizontalAlignment="Center"
								   Source="{Binding Path=MediumImageSource}"
								   VerticalAlignment="Center"
								   Width="18"/>
						</Grid>
					</DataTemplate>
				</Setter.Value>
			</Setter>
			<Setter Property="ItemContainerStyle">
				<Setter.Value>
					<Style TargetType="{x:Type ComboBoxItem}">
						<Setter Property="HorizontalContentAlignment"
								Value="Left"/>
						<Setter Property="VerticalContentAlignment"
								Value="Center"/>
						<Setter Property="Template">
							<Setter.Value>
								<ControlTemplate TargetType="{x:Type ComboBoxItem}">
									<StackPanel Margin="1"
												Name="StackPanel"
												Orientation="Horizontal">
										<Image Height="18"
											   Name="Image"
											   Source="{Binding Path=MediumImageSource}"
											   VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
											   Width="18"/>
										<TextBlock Margin="4,0,4,0"
												   Text="{Binding Path=Description}"
												   VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
									</StackPanel>
									<ControlTemplate.Triggers>
										<Trigger Property="IsHighlighted"
												 Value="true">
											<Setter Property="Background"
													TargetName="StackPanel"
													Value="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}"/>
											<Setter Property="Foreground"
													Value="{DynamicResource {x:Static SystemColors.HighlightTextBrushKey}}"/>
										</Trigger>
									</ControlTemplate.Triggers>
								</ControlTemplate>
							</Setter.Value>
						</Setter>
					</Style>
				</Setter.Value>
			</Setter>
			<Setter Property="VerticalContentAlignment"
					Value="Center"/>
		</Style>
	</ViewContainer.Resources>
	<!-- ColumnView -->
	<ColumnView AllowsColumnReorder="true"
				ItemContainerStyle="{StaticResource ColumnViewItemContainerStyleKey}">
		<!-- Status -->
		<ColumnViewColumn Description="Status"
						  HeaderContainerStyle="{StaticResource ImageHeaderStyle}"
						  HeaderHorizontalAlignment="Center"
						  MinWidth="24"
						  Ordinal="0"
						  SortPath="StatusCode"
						  Width="24">
			<ColumnViewColumn.Filters>
				<FilterItem Content="New"
							GroupName="StatusGroup"
							x:Name="FilterStatusNew"/>
				<FilterItem Content="Active"
							GroupName="StatusGroup"
							x:Name="FilterStatusActive"/>
				<FilterItem Content="Partially Filled"
							GroupName="StatusGroup"
							x:Name="FilterStatusPartiallyFilled"/>
				<FilterItem Content="Filled"
							GroupName="StatusGroup"
							x:Name="FilterStatusFilled"/>
			</ColumnViewColumn.Filters>
			<ColumnViewColumn.Header>
				<Image Height="20"
					   HorizontalAlignment="Center"
					   Source="{Binding Path=MediumImage,Source={StaticResource StatusIcon}}"
					   VerticalAlignment="Center"
					   Width="20"/>
			</ColumnViewColumn.Header>
			<ColumnViewColumn.CellTemplate>
				<DataTemplate>
					<asset:StatusSelector Style="{StaticResource ItemSelectorStyle}">
						<asset:StatusSelector.ContentTemplate>
							<DataTemplate>
								<Image Height="18"
									   Source="{Binding Path=MediumImageSource}"
									   Width="18"/>
							</DataTemplate>
						</asset:StatusSelector.ContentTemplate>
					</asset:StatusSelector>
				</DataTemplate>
			</ColumnViewColumn.CellTemplate>
		</ColumnViewColumn>
		<!-- Security Name -->
		<ColumnViewColumn Description="Security Name"
						  Ordinal="1"
						  Header="Security Name"
						  SortPath="SecurityName"
						  Width="213">
			<ColumnViewColumn.CellTemplate>
				<DataTemplate>
					<ContentControl Content="{Binding Path=SecurityName}"
									Style="{StaticResource LeftTextStyle}"/>
				</DataTemplate>
			</ColumnViewColumn.CellTemplate>
		</ColumnViewColumn>
		<!-- CUSIP -->
		<ColumnViewColumn Description="CUSIP"
						  Header="CUSIP"
						  MinWidth="20"
						  Ordinal="2"
						  SortPath="Symbol"
						  Width="73">
			<ColumnViewColumn.Filters>
				<FilterItem Content="A - G"
							GroupName="SymbolGroup"
							x:Name="FilterSymbolAtoG"/>
				<FilterItem Content="H - M"
							GroupName="SymbolGroup"
							x:Name="FilterSymbolHtoM"/>
				<FilterItem Content="N - R"
							GroupName="SymbolGroup"
							x:Name="FilterSymbolNtoR"/>
				<FilterItem Content="S - Z"
							GroupName="SymbolGroup"
							x:Name="FilterSymbolStoZ"/>
			</ColumnViewColumn.Filters>
			<ColumnViewColumn.CellTemplate>
				<DataTemplate>
					<ContentControl Content="{Binding Path=Symbol}"
									Style="{StaticResource LeftTextStyle}"/>
				</DataTemplate>
			</ColumnViewColumn.CellTemplate>
		</ColumnViewColumn>
		<!-- Side Code -->
		<ColumnViewColumn Description="Side"
						  Header="Side"
						  HeaderHorizontalAlignment="Center"
						  MinWidth="36"
						  Ordinal="3"
						  SortPath="SideCode"
						  Width="36">
			<ColumnViewColumn.Filters>
				<FilterItem Content="Buy"
							GroupName="SideGroup"
							x:Name="FilterSideBuy"/>
				<FilterItem Content="Buy Cover"
							GroupName="SideGroup"
							x:Name="FilterSideBuyCover"/>
				<FilterItem Content="Sell"
							GroupName="SideGroup"
							x:Name="FilterSideSell"/>
				<FilterItem Content="Sell Short"
							GroupName="SideGroup"
							x:Name="FilterSideSellShort"/>
			</ColumnViewColumn.Filters>
			<ColumnViewColumn.CellTemplate>
				<DataTemplate>
					<asset:SideComboBox ItemsSource="{Binding Path=SideItems,RelativeSource={RelativeSource Self}}"
										SelectedValue="{Binding Path=SideCode}"
